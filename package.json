{
  "name": "eindhoven",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:html": "rimraf build/*.htlm",
    "clean": "rimraf build/* && mkdirp build/js",
    "build:html": "copyfiles -u 1 src/index.html build/",
    "build:js": "browserify src/js/index.js -t [ babelify --presets [ es2015 ] ] | uglifyjs -o build/js/bundle.js",
    "build": "npm run clean && npm run build:html && npm run build:js",
    "watch:js": "watchify  src/js/index.js -o build/js/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watch:html": "watch 'npm run build:html' src -p /js/",
    "watch": "npm run watch:js & npm run watch:html",
    "serve": "npm run build && live-server build/ & npm run watch",
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "copyfiles": "^1.2.0",
    "live-server": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.27",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  }
}
