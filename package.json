{
  "name": "qproto",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean:html": "rimraf build/*.htlm",
    "clean": "rimraf build/* && mkdirp build/js && mkdirp build/css",
    "build:html": "copyfiles -u 1 src/index.html build/",
    "build:js": "browserify src/js/index.js -t [ babelify --presets [ es2015 ] ] | uglifyjs -o build/js/bundle.js",
    "build:css": "stylus src/styles/main.styl -o build/css/main.css",
    "prebuild": "npm run clean",
    "build": "npm run build:html && npm run build:js && npm run build:css",
    "watch:js": "watchify  src/js/index.js -o build/js/bundle.js -t [ babelify --presets [ es2015 ] ]",
    "watch:html": "watch 'npm run build:html' src -p /js,styles/",
    "watch:css": "npm run build:css -- -w",
    "watch": "npm run watch:js & npm run watch:html & npm run watch:css",
    "preserve": "npm run build",
    "serve": "npm run watch & live-server build/",
    "start": "npm run serve",
    "dist": "rimraf dist && npm run build && ncp build dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Dario Fuzinato",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "copyfiles": "^1.2.0",
    "live-server": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.5.0",
    "ncp": "^2.0.0",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "uglify-js": "^3.0.27",
    "watch": "^1.0.2",
    "watchify": "^3.9.0"
  }
}
